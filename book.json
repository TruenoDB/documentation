{
    "gitbook": ">=3.2.1",
    "plugins": ["edit-link", "prism", "-highlight", "github", "anchorjs", "codetabs", "expandable-chapters", "include-codeblock", "custom-favicon"],
    "pluginsConfig": {
        "favicon": "assets/images/favicon.ico",
        "include-codeblock": {
            "template": "{{{content}}}"
        },
        "edit-link": {
            "base": "https://github.com/TruenoDB/documentation/tree/master",
            "label": "Edit This Page"
        },
        "github": {
            "url": "https://github.com/TruenoDB/documentation/"
        },
        "theme-default": {
            "styles": {
                "website": "styles/website.css"
            }
        }
    },
    "title": "Trueno"
}